#ifndef _TBLIS_TBLIS_CONFIG_H
#define _TBLIS_TBLIS_CONFIG_H 1

/* define if the compiler supports basic C++20 syntax */
#cmakedefine TBLIS_HAVE_CXX20 1

/* Define to 1 if you have the <dlfcn.h> header file. */
#cmakedefine TBLIS_HAVE_DLFCN_H 1

/* Define to 1 if you have the <hbwmalloc.h> header file. */
#cmakedefine TBLIS_HAVE_HBWMALLOC_H 1

/* Define to 1 if you have the <hwloc.h> header file. */
#cmakedefine TBLIS_HAVE_HWLOC_H 1

/* Define to 1 if you have the <inttypes.h> header file. */
#cmakedefine TBLIS_HAVE_INTTYPES_H 1

/* Define if the system has the lscpu command. */
#cmakedefine TBLIS_HAVE_LSCPU 1

/* Define to 1 if you have the <memkind.h> header file. */
#cmakedefine TBLIS_HAVE_MEMKIND_H 1

/* Define to 1 if you have the <stdint.h> header file. */
#cmakedefine TBLIS_HAVE_STDINT_H 1

/* Define to 1 if you have the <stdio.h> header file. */
#cmakedefine TBLIS_HAVE_STDIO_H 1

/* Define to 1 if you have the <stdlib.h> header file. */
#cmakedefine TBLIS_HAVE_STDLIB_H 1

/* Define to 1 if you have the <strings.h> header file. */
#cmakedefine TBLIS_HAVE_STRINGS_H 1

/* Define to 1 if you have the <string.h> header file. */
#cmakedefine TBLIS_HAVE_STRING_H 1

/* Define to 1 if you have the 'sysconf' function. */
#cmakedefine TBLIS_HAVE_SYSCONF 1

/* Define to 1 if you have the 'sysctl' function. */
#cmakedefine TBLIS_HAVE_SYSCTL 1

/* Define to 1 if you have the 'sysctlbyname' function. */
#cmakedefine TBLIS_HAVE_SYSCTLBYNAME 1

/* Define to 1 if you have the <sys/stat.h> header file. */
#cmakedefine TBLIS_HAVE_SYS_STAT_H 1

/* Define to 1 if you have the <sys/types.h> header file. */
#cmakedefine TBLIS_HAVE_SYS_TYPES_H 1

/* Define to 1 if you have the <unistd.h> header file. */
#cmakedefine TBLIS_HAVE_UNISTD_H 1

/* sysconf(_SC_NPROCESSORS_CONF) is valid. */
#cmakedefine TBLIS_HAVE__SC_NPROCESSORS_CONF 1

/* sysconf(_SC_NPROCESSORS_ONLN) is valid. */
#cmakedefine TBLIS_HAVE__SC_NPROCESSORS_ONLN 1

/* label_type */
#cmakedefine TBLIS_LABEL_TYPE @TBLIS_LABEL_TYPE@

/* len_type */
#cmakedefine TBLIS_LEN_TYPE @TBLIS_LEN_TYPE@

/* Name of package */
#cmakedefine TBLIS_PACKAGE "@TBLIS_PACKAGE@"

/* Define to the address where bug reports for this package should be sent. */
#cmakedefine TBLIS_PACKAGE_BUGREPORT "@TBLIS_PACKAGE_BUGREPORT@"

/* Define to the full name of this package. */
#cmakedefine TBLIS_PACKAGE_NAME "@TBLIS_PACKAGE_NAME@"

/* Define to the full name and version of this package. */
#cmakedefine TBLIS_PACKAGE_STRING "@TBLIS_PACKAGE_STRING@"

/* Define to the one symbol short name of this package. */
#cmakedefine TBLIS_PACKAGE_TARNAME "@TBLIS_PACKAGE_TARNAME@"

/* Define to the home page for this package. */
#cmakedefine TBLIS_PACKAGE_URL "@TBLIS_PACKAGE_URL@"

/* Define to the version of this package. */
#cmakedefine TBLIS_PACKAGE_VERSION "@TBLIS_PACKAGE_VERSION@"

/* More convenient macro for restrict. */
#cmakedefine TBLIS_RESTRICT @TBLIS_RESTRICT@

/* Define to 1 if all of the C89 standard headers exist (not just the ones
   required in a freestanding environment). This macro is provided for
   backward compatibility; new code need not use it. */
#cmakedefine TBLIS_STDC_HEADERS 1

/* stride_type */
#define TBLIS_STRIDE_TYPE @TBLIS_STRIDE_TYPE@

/* The top source directory. */
#define TBLIS_TOPDIR "@TBLIS_TOPDIR@"

/* Version number of package */
#define TBLIS_VERSION "@TBLIS_VERSION@"

/* Major version number. */
#define TBLIS_VERSION_MAJOR @TBLIS_VERSION_MAJOR@

/* Minor version number. */
#define TBLIS_VERSION_MINOR @TBLIS_VERSION_MINOR@

/* Patch version number. */
#define TBLIS_VERSION_PATCH @TBLIS_VERSION_PATCH@

/* once: _TBLIS_TBLIS_CONFIG_H */
#endif
